let arr = [5, 23, 8, 14, 15];
let n = arr.length;

console.log("Arreglo inicial:", arr.join(", "));

for (let i = 1; i < n; i++) {
    let key = arr[i];
    let j = i - 1;

    while (j >= 0 && arr[j] > key) {
        arr[j + 1] = arr[j];
        j--;
    }
    arr[j + 1] = key;

    console.log(`\n--- Inserci√≥n ${i} ---`);
    console.log(arr.join(", "));
}

console.log("\nArreglo ordenado:", arr.join(", "));
