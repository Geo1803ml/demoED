arr = [25, 43, 58, 16, 9]
n = len(arr)

print("Arreglo inicial:", arr)

for i in range(1, n):
    key = arr[i]
    j = i - 1

    while j >= 0 and arr[j] > key:
        arr[j + 1] = arr[j]
        j -= 1
    arr[j + 1] = key

    print(f"\n--- InserciÃ³n {i} ---")
    print(arr)

print("\nArreglo ordenado:", arr)
