arr = [75, 35, 19, 64, 21]
n = len(arr)

print("Arreglo inicial:", arr)

for i in range(n - 1):
    min_index = i

    for j in range(i + 1, n):
        if arr[j] < arr[min_index]:
            min_index = j

    arr[i], arr[min_index] = arr[min_index], arr[i]

    print(f"\n--- SelecciÃ³n {i+1} ---")
    print(arr)

print("\nArreglo ordenado:", arr)
